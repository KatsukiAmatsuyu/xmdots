{
	"meta": {
		"name": "Horizontal Server List",
		"author": "Gibbu#1211",
		"version": "2.1.0",
		"description": "Moves the server list from the left to the top of Discord.",
		"source": "https://github.com/DiscordStyles/HorizontalServerList",
		"invite": "ZHthyCw",
		"website": "https://gibbu.me"
	},
	"compiler": {
		"prefix": true
	},
	"dev": {
		"target": ["src", "dev.scss"],
		"output": ["HorizontalServerList.theme.css"]
	},
	"dist": {
		"target": ["src", "dist.scss"],
		"output": ["dist", "HorizontalServerList.theme.css"]
	},
	"base": {
		"target": ["src", "base.scss"],
		"output": ["dist", "dist", "HorizontalServerList.css"]
	},
	"addons": [
		{
			"target": ["src", "addons", "_bottomhsl.scss"],
			"output": ["dist", "bottomhsl.css"]
		}
	]
}